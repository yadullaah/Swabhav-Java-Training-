package com.fascadedvd.model;

public class DVDPlayer implements ISystem, IDevice {

	@Override
	public void on() {
		System.out.println("The device is switched on");

	}

	@Override
	public void off() {
		System.out.println("The device is switched off");

	}

	@Override
	public void start() {
		System.out.println("The system has started");

	}

	@Override
	public void stop() {
		System.out.println("The system has stopped");
	}

}
